"list-complete-item\">\n          <div class=\"list-complete-item-handle\">{{element.name}}</div>\n          <div>\n            <i class=\"el-icon-delete\" @click=\"handleDel(index, element.id)\"></i>\n          </div>\n        </div>\n      </draggable>\n    </div>\n    <div style=\"width: 100%; height: 10px; background-color: #bfbfbf\"></div>\n    <div class=\"dndList-list\">\n      <h3>所有</h3>\n      <draggable\n        :list=\"list2\"\n        :options=\"{group:{name: falgs,pull:'clone'},filter: '.undraggable', sort: false}\"\n        @end=\"end\"\n        class=\"dragArea\"\n      >\n        <div\n          v-for=\"element in list2\"\n          :key=\"element.id\"\n          :class=\"{undraggable : element.flag}\"\n          class=\"list-complete-item\"\n        >\n          <div class=\"list-complete-item-handle2\">{{element.name}}</div>\n        </div>\n      </draggable>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { Component, Prop, Vue } from \"vue-property-decorator\";\nimport draggable from \"vuedraggable\";\n@Component({\n  components: {\n    draggable\n  }\n})\nexport default class Mains extends Vue {\n  falgs = \"article\";\n  disabled = false;\n  list1 = [];\n  list2 = [\n    { id: 1, name: 1 },\n    { id: 2, name: 2 },\n    { id: 3, name: 3 },\n    { id: 4, name: 4 },\n    { id: 5, name: 5 },\n    { id: 6, name: 6 },\n    { id: 7, name: 7 },\n    { id: 8, name: 8 },\n    { id: 9, name: 9 },\n    { id: 10, name: 10 }\n  ];\n  end(ev: any) {\n    if (ev.to.className === \"dragArea11\") {\n      this.$set(this.list2[ev.oldIndex], \"flag\", true);\n    }\n  }\n  start22(event: any) {\n    this.falgs = \"222222\";\n  }\n  end22(ev: any) {\n    this.falgs = \"article\";\n  }\n  handleDel(index: any, id: any) {\n    this.list1.splice(index, 1);\n    const q: any = this.list2.find((value, index, arr) => {\n      return value.id === id;\n    });\n    this.$set(q, \"flag\", false);\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang='less' scoped>\n\n  .list-complete-item {\n    cursor: pointer;\n    position: relative;\n    font-size: 14px;\n    padding: 5px 12px;\n    display: inline-block;\n    margin-right: 20px;\n    width: 50px;\n    height: 50px;\n    border: 1px solid #bfcbd9;\n    transition: all 1s;\n  }\n \n  .list-complete-item.sortable-chosen {\n    background: #4AB7BD;\n  }\n \n  .list-complete-item.sortable-ghost {\n    background: #30B08F;\n  }\n  .undraggable {\n    background-color: red;\n  }\n \n  .list-complete-enter,\n  .list-complete-leave-active {\n    opacity: 0;\n  }\n</style>\n"]}]}